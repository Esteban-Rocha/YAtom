  editor:
    fontFamily: "Hack"
    invisibles: {}
    showIndentGuide: true
    tabType: "soft"
    zoomFontWhenCtrlScrolling: true
  emmet:
    useEmmetComments: false
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  "find-and-replace":
    focusEditorAfterSearch: true
    openProjectFindResultsInRightPane: true
    scrollToResultOnLiveSearch: true
  "fuzzy-finder":
    preserveLastSearch: true
  "gist-it":
    newGistsDefaultToPrivate: true
  "git-blame":
    ignoreWhiteSpaceDiffs: true
  "git-blame-plus": {}
  "git-diff":
    showIconsInEditorGutter: true
  "git-history":
    showDiff: false
  "git-log":
    fontScale: 0.9
  "git-projects":
    ignoredPatterns: "node_modules;.git;bower_components;.statictmp"
    maxDepth: 1
  "highlight-column":
    lineMode: true
  "highlight-line":
    enableSelectionBorder: true
  "highlight-selected":
    onlyHighlightWholeWords: false
  "indentation-indicator":
    indicatorPosition: "right"
  jumpy:
    useHomingBeaconEffectOnJumps: false
  "language-babel":
    transpileOnSave: false
  linter:
    lintOnFly: false
    showErrorTabFile: false
    showErrorTabProject: false
    statusBar: "Show all errors"
  "linter-eslint":
    useGlobalEslint: true
  "linter-jscs":
    displayAs: "jscs Warning"
    esnext: true
    fixOnSave: true
    onlyConfig: true
    preset: "<none>"
  "linter-sass-lint":
    globalSassLint: true
    noConfigDisable: true
  "linter-stylelint":
    disableWhenNoConfig: true
  "markdown-preview":
    useGitHubStyle: true
  "markdown-writer":
    siteEngine: "hexo"
  minimap:
    adjustMinimapWidthToSoftWrap: false
    plugins:
      bookmarks: true
      codeglance: true
      "find-and-replace": true
      "git-diff": true
      "highlight-selected": true
      "minimap-autohide": false
      "minimap-hide": true
      pigments: false
      selection: true
      "split-diff": true
  "minimap-selection":
    highlightCursorsLines: true
  "node-debugger":
    nodePath: "/usr/local/bin/node"
  pigments:
    autocompleteScopes: [
      ".yss"
    ]
    groupPaletteColors: "by file"
    markerType: "dot"
    sortPaletteColors: "by color"
  "preview-plus":
    cjsx: "JavaScript"
    css: "SCSS"
    html: "browser"
  "recent-files-fuzzy-finder":
    restoreSession: true
  "relative-line-numbers":
    showNormalLineNumbers: false
  "release-notes":
    viewedVersion: "0.73.0"
  "spell-check":
    grammars: [
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
    ]
  "split-diff":
    ignoreWhitespace: true
  tabs:
    usePreviewTabs: true
  "terminal-plus":
    core: {}
    style:
      animationSpeed: 2
      fontFamily: "Anonymous Pro"
      theme: "homebrew"
  "todo-show":
    sortBy: "File"
  "tool-bar":
    iconSize: "16px"
    position: "Left"
  "trailing-spaces":
    enableForCursorLines: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".gfm.source":
  editor:
    softWrap: false
".json.source":
  editor:
    softWrap: false
